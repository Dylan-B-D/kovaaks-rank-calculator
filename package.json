{
  "name": "kovaaks-rank-api",
  "version": "1.0.0",
  "description": "A rank calculation system for KovaaK's benchmarks with CLI executable and Python bindings",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Dylan-B-D/kovaaks-rank-calculator"
  },
  "scripts": {
    "build": "tsc && pkg dist/cli/kovaaks-rank-cli.js --targets node18-win-x64 --output output/kovaaks-rank-cli.exe",
    "build:windows": "tsc && pkg dist/cli/kovaaks-rank-cli.js --targets node18-win-x64 --output output/kovaaks-rank-cli.exe",
    "build:linux": "tsc && pkg dist/cli/kovaaks-rank-cli.js --targets node18-linux-x64 --output output/kovaaks-rank-cli",
    "build:macos": "tsc && pkg dist/cli/kovaaks-rank-cli.js --targets node18-macos-x64 --output output/kovaaks-rank-cli"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "pkg": "^5.8.1",
    "typescript": "^5.9.3"
  }
}